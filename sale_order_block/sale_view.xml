<openerp>
  <data>
    <template id="index">
      <t t-call="website.layout">
        <title>kodspecialisterna</title>
        <t t-foreach="active_orders" t-as="active_order">
          <div>
            <h3 t-field='active_order.partner_id.name' />
            <p t-esc="active_order.name" />
          
          </div>
        </t>
      </t>
    </template>
    <template id="page_behandling" name="Category Page">
      <t t-call="website.layout">
        <div id="wrap">
          <section>
            <div class="container mt16 mb64" style="width: 100%;">
              <div class="col-md-12" style="padding: 0px; margin: auto;">
                <div t-field="category.website_description" />
                <table id="pro_table" class="table table-bordered table-responsive" style="border: none;">
                  <t t-foreach="products" t-as="p">
                    <tbody>
                      <tr>
                        <td>
                          <h3 style="color: #B9A55A;">
                            <tspan t-field="p.name" />
                          </h3>
                          <span t-field="p.description" />
                        </td>
                        <td>
                          â€‹
                          <p />
                          <span t-field="p.list_price" style="white-space: nowrap;" t-field-options="{&quot;widget&quot;:&quot;monetary&quot;,&quot;display_currency&quot;: &quot;user_id.partner_id.property_product_pricelist.currency_id&quot;}" />
                        </td>
                      </tr>
                    </tbody>
                  </t>
                </table>
              </div>
            </div>
          </section>
          <t t-raw="0" />
          <div class="oe_structure oe_empty" />
        </div>
      </t>
    </template>
    <template id="page_order" name="Order page test">
      <t t-call="website.layout">
        <div id="wrap">
          <section>
            <div class="container mt16 mb64" style="width: 100%;">
              <div class="col-md-12" style="padding: 0px; margin: auto;">
                <div t-field="order.name" />
                <div t-field='order.note' />
                <div t-field='order.partner_id.name' />
              </div>
            </div>
          </section>
          <t t-raw="0" />
          <div class="oe_structure oe_empty" />
        </div>
      </t>
    </template>
