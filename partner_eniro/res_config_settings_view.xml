<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="res_config_settings_view_form" model="ir.ui.view">
    <field name="model">res.config.settings</field>
    <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//div[@name='integration']" position="inside">
        <div id="eniro" class="col-xs-12 col-md-6 o_setting_box">
          <div class="o_setting_left_pane">
          </div>
          <div class="o_setting_right_pane" name="critical_path_duration_base_right_pane">
            <label for="eniro_api_profile" string="Eniro"/>
            <div class="text-muted">
                Profile
            </div>
            <field name="eniro_api_profile" class="o_light_label"/>
            <div class="text-muted">
                Key
            </div>
            <field name="eniro_api_key" class="o_light_label"/>
          </div>
        </div>
      </xpath>
    </field>
  </record>
</odoo>
